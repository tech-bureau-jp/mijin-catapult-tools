    "@types/jest": "^29.2.0",
    "@types/node": "^14.14.31",
    "@types/node-forge": "^1.3.0",
    "@types/tough-cookie": "^4.0.2",
    "bufferutil": "^4.0.3",
    "jest": "^29.2.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^8.0.17",
    "typescript": "^4.3.5",
    "utf-8-validate": "^5.0.4",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
